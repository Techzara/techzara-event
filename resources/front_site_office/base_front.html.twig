{% set current_route = app.request.attributes.get('_route') %}

<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet" media="screen" />
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Comfortaa:300,400,700|Maven+Pro:400,500,700,900">
            <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Montserrat">
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/font-awesome.min.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/et-line.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/ionicons.min.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/owl.carousel.min.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/owl.theme.default.min.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/animate.min.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/main.css') }}">
            <link rel="stylesheet" href="{{ asset('front_site_office/css/custom.css') }}">
        {% endblock %}

        <link rel="shortcut icon" href="{{ asset('front_site_office/img/logo/icon.jpeg') }}"/>
    </head>

    <body id="id-home" class="kl-front-site">
        <div class="loader">
            <div class="loader-outter"></div>
            <div class="loader-inner"></div>
        </div>
        <div class="wrapper">
            <!-- Header -->
            {{ include("FrontSiteBundle:Template:base_front_header.html.twig") }}
            <!-- Main content -->
                {% block body %}{% endblock %}
            <!-- Footer -->
            {{ render(controller('FrontSiteBundle:TzeFooter:show')) }}
        </div>

        {% block javascripts %}
            <script src="{{ asset('front_site_office/js/jquery.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/popper.js') }}"></script>
            <script src="{{ asset('front_site_office/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/waypoints.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/owl.carousel.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/parallax.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/jquery.counterup.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/jquery.countdown.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/wow.min.js') }}"></script>
            <script src="{{ asset('front_site_office/js/main.js') }}"></script>
        {% endblock %}

    </body>
</html>